<?php if($cycle->designation->tax): ?>
    <div class="text-left"><strong><small>Tax Hours | <?php echo e($cycle->tax); ?> / <?php echo e($cycle->designation->tax); ?></small></strong></div>
    <div class="progress">
        <div class="progress-bar progress-bar-primary progress-bar-striped active" role="progressbar"
             aria-valuenow="<?php echo e(($cycle->designation->tax > 1) ? $cycle->tax * 100 / $cycle->designation->tax : 0); ?>"
             aria-valuemin="0" aria-valuemax="100" style="width: <?php echo e(($cycle->designation->tax > 1) ? ($cycle->tax) ? $cycle->tax * 100 /$cycle->designation->tax : 10 : '2em'); ?>%">
            <?php echo e(round(($cycle->designation->tax > 1) ? $cycle->tax * 100 / $cycle->designation->tax : 0)); ?>%
        </div>
    </div>
<?php endif; ?>

