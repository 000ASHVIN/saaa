                    <?php echo $__env->make('webinars_on_demand.includes.video', array_except(get_defined_vars(), array('__data', '__path')))->render(); ?>
                <?php endforeach; ?>
            <?php endforeach; ?>
        </div>
    <?php else: ?>
        <div class="alert alert-info no-webinars">
            <p><strong>Note..</strong></p>
            <p>Your search returned <?php echo e(count($videos)); ?> results, please check your search and try again.</p>
